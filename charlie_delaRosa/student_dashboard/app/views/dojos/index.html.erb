<h1>Listing All Dojos</h1>

<table>
  <tr>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Actions</th>
  </tr>
  <% Dojo.all.each do |dojo| %>
  <tr>
    <td><%= dojo.branch %></td>
    <td><%= dojo.street %></td>
    <td><%= dojo.city %></td>
    <td><%= dojo.state %></td>
    <td><%= link_to "Show", dojo %></td>
    <td><%= link_to "Edit", edit_dojo_path(dojo) %></td>
    <td><%= link_to "Delete", dojo_delete_path(dojo), method: :delete, data: {confirm: "Are you sure?"} %></td>
    <!-- <td><a href="dojos/:id">Show</a><a href="dojos/:id/edit">Edit</a>
      <a href="dojos/:id" method = delete>Destroy</a></td> -->
  </tr>
  <% end %>
</table>

<a href="/dojos/new">New Dojo</a>
